<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

    <class name="bioinfo.comaWebServer.entities.AlignmentFilter" table="AlignmentFilter">   
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>
        
        <property name="lc_u" column="lc_u" /> 
        <property name="lc_w" column="lc_w" />
        <property name="lc_f" column="lc_f" /> 
        <property name="uc_F" column="uc_F" />
    </class>
    
</hibernate-mapping>
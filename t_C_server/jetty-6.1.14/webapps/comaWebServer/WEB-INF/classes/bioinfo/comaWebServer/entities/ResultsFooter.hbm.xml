<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

    <class name="bioinfo.comaWebServer.entities.ResultsFooter" table="ResultsFooter">   
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>

        <property name="scoringMethod"              />
        <property name="gapOpenCost"                />
        <property name="initialGapExtensionCost"    />
        <property name="deletionProbabilityWeight"  />  
        <property name="ungappedK"                  />
        <property name="ungappedLambda"             />
        <property name="gappedK"                    />
        <property name="gappedLambda"               />
        <property name="queryLength"                />  
        <property name="databaseLength"             />
        <property name="queryEffectiveLength"       />
        <property name="databaseEffectiveLength"    />  
        <property name="effectiveSearchSpace"       />
        
    </class>
    
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

    <class name="bioinfo.comaWebServer.entities.HSSegmentPairs" table="HSSegmentPairs"> 
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>
        
        <property name="hsplen" column="hsplen" />
        <property name="hsminscore" column="hsminscore" />
        <property name="hsmaxdist" column="hsmaxdist" />  
        <property name="nohsps" column="nohsps" /> 
    </class>
    
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="bioinfo.comaWebServer.entities.DatabaseItem" table="DatabaseItem">	
		<id name="id" column="id" >
			<generator class="increment"/>
		</id>
		
		<property name="name"  />
		<property name="type"  />
		<property name="path"  />	
		<property name="value" />
		<property name="partialName"  /> 
        <property name="noModeling"   />
	</class>
	
</hibernate-mapping>
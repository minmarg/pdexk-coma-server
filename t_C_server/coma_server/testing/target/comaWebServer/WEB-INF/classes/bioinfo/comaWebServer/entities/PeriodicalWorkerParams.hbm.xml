<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

    <class name="bioinfo.comaWebServer.entities.PeriodicalWorkerParams" table="PeriodicalWorkerParams"> 
        <id name="id" column="id" >
            <generator class="increment"/>
        </id>
        
        <property name="jobExpiration"              />
        <property name="jobWithErrorsExpiration"    />
        <property name="evalueForPictures"          />
        <property name="numberForPictures"          />  
        
        <property name="redThreshold"          /> 
        <property name="greenThreshold"        />  
    </class>
    
</hibernate-mapping>
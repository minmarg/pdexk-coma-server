<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="bioinfo.comaWebServer.entities.ProfileConstruction" table="ProfileConstruction">	
		<id name="id" column="id" >
			<generator class="increment"/>
		</id>
		
		<property name="lc_t" column="lc_t" />
		<property name="lc_s" column="lc_s" />	
		
		<property name="lc_b" column="lc_b" />
        <property name="uc_Y" column="uc_Y" />  
        <property name="lc_x" column="lc_x" />
	</class>
	
</hibernate-mapping>